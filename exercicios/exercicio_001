// Calculadora básica

#include <stdio.h>

int main()
{       
        printf("************************\n*        FAM           *\n* CENTRO UNIVERSITARIO *  \n************************\n\n");
        
        int inteiro1,inteiro2,result;
        
        printf("Digite um numero inteiro:\n");
        scanf("%d",&inteiro1);
        
        printf("Digite outro número inteiro:\n");
        scanf("%d",&inteiro2);
        
        result = inteiro1 + inteiro2;
        
        printf("O resultado é: %d + %d = %d",inteiro1,inteiro2,result);
        
        return 0;
        
        
}