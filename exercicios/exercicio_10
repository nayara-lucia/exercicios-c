#include <stdio.h>

// Programa mostra se vc está apto para votar

int main()
{

int ano_nascimento, idade;
printf ("Informe o ano de seu nascimento: \n");
scanf ("%d",&ano_nascimento);

idade =  2023 - ano_nascimento;

    if(ano_nascimento < 2007){
    
        printf("Você tem %d anos, já pode votar", idade);
    }
   
    else {
    
    printf("Você tem %d anos, ainda não pode votar",idade);

}
    return 0;

        
    }