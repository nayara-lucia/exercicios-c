#include <stdio.h>

// Usuario digita dois numeros, programa mostra qual é o maior

int main()
{

int n1, n2;

printf("Digite o primeiro número: ");
scanf("%d",&n1);

printf("Digite o segundo número: ");
scanf("%d",&n2);

if(n1 > n2){
    
    printf("O número %d, é maior que %d",n1,n2);
    
    }

    else {
   
    printf("O número %d, é maior que %d",n2,n1);

}

return 0;
        
}